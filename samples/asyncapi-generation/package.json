{
  "name": "@amqp-contract-samples/asyncapi-generation",
  "private": true,
  "description": "AsyncAPI generation sample using amqp-contract",
  "type": "module",
  "scripts": {
    "bundle": "asyncapi bundle asyncapi.json -o asyncapi-bundle.json",
    "generate": "tsx src/index.ts",
    "typecheck": "tsc --noEmit",
    "validate": "asyncapi validate asyncapi.json"
  },
  "dependencies": {
    "@amqp-contract/asyncapi": "workspace:*",
    "@amqp-contract/contract": "workspace:*",
    "@orpc/zod": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@amqp-contract/tsconfig": "workspace:*",
    "@asyncapi/cli": "catalog:",
    "@types/node": "catalog:",
    "tsx": "catalog:",
    "typescript": "catalog:"
  }
}
