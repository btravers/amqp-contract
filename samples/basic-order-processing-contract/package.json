{
  "name": "@amqp-contract-samples/basic-order-processing-contract",
  "private": true,
  "description": "Contract definition for order processing sample",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "scripts": {
    "build": "tsdown src/index.ts --dts",
    "generate:asyncapi:json": "tsx scripts/generate-asyncapi-json.ts",
    "generate:asyncapi:yaml": "tsx scripts/generate-asyncapi-yaml.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@amqp-contract/contract": "workspace:*",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@amqp-contract/asyncapi": "workspace:*",
    "@amqp-contract/tsconfig": "workspace:*",
    "@orpc/zod": "catalog:",
    "@types/node": "catalog:",
    "tsdown": "catalog:",
    "tsx": "catalog:",
    "typescript": "catalog:",
    "yaml": "catalog:"
  }
}
