{
  "name": "@amqp-contract/client-nestjs",
  "version": "0.2.1",
  "description": "NestJS integration for @amqp-contract/client",
  "keywords": [
    "amqp",
    "client",
    "contract",
    "nestjs",
    "rabbitmq",
    "typescript"
  ],
  "homepage": "https://github.com/btravers/amqp-contract#readme",
  "bugs": {
    "url": "https://github.com/btravers/amqp-contract/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/btravers/amqp-contract.git",
    "directory": "packages/client-nestjs"
  },
  "license": "MIT",
  "author": "Benoit TRAVERS <benoit.travers.fr@gmail.com>",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist",
    "docs"
  ],
  "scripts": {
    "build": "tsdown src/index.ts --format cjs,esm --dts --clean",
    "build:docs": "typedoc",
    "dev": "tsdown src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest run --project unit",
    "test:watch": "vitest --project unit",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@amqp-contract/client": "workspace:*",
    "@amqp-contract/contract": "workspace:*"
  },
  "devDependencies": {
    "@amqp-contract/tsconfig": "workspace:*",
    "@amqp-contract/typedoc": "workspace:*",
    "@nestjs/common": "catalog:",
    "@swan-io/boxed": "catalog:",
    "@types/amqplib": "catalog:",
    "@types/node": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "amqp-connection-manager": "catalog:",
    "amqplib": "catalog:",
    "reflect-metadata": "catalog:",
    "rxjs": "catalog:",
    "tsdown": "catalog:",
    "typedoc": "catalog:",
    "typedoc-plugin-markdown": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:",
    "zod": "catalog:"
  },
  "peerDependencies": {
    "@nestjs/common": ">=11.0.0",
    "@nestjs/core": ">=11.0.0",
    "reflect-metadata": ">=0.1.13",
    "rxjs": ">=7.0.0"
  }
}
